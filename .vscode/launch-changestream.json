{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/change_stream.js",
            "args": [
            ]
        },
        {
            "type": "node-terminal",
            "name": "Run Script: change-stream",
            "request": "launch",
            "command": "node --introspect ./src/change_stream.js",
            "cwd": "${workspaceFolder}"
        }
    ]
}
